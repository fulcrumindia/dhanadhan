<!--   Big container   -->
<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <!--      Wizard container        -->
            <div class="wizard-container">
                <h3 class="w3ls-title w3ls-title1">{{wizarddata.header}}</h3>
                <div class="card wizard-card" data-color="{{wizarddata.color}}" id="wizardProfile">
                    <form action="/" method="get">
                        <!--        You can switch " data-color="purple" "  with one of the next bright colors: "green", "orange", "red", "blue"       -->

                        <div class="wizard-header">
                            <h3 class="wizard-title">
                                {{wizarddata.title}}
                            </h3>
                            <h5>{{wizarddata.description}}</h5>
                        </div>
                        <div class="wizard-navigation">
                            <ul>
                                {{#each wizarddata.tabs}}
                                <li><a href="#{{href}}" data-toggle="tab">{{name}}</a></li>
                                {{/each}}
                            </ul>
                        </div>

                        <div class="tab-content">
                            {{#each wizarddata.tabs}}
                            <div class="tab-pane" id="{{href}}">
                                <div class="row">
                                    <h4 class="info-text"> {{description}}</h4>
                                    {{#if src}}
                                        {{#each (lookup ../this src)}}
                                            {{#each ../fields}}
                                                {{>listbusiness-body addInfo=../name value=../name catIcon=../icon}}
                                            {{/each}}
                                        {{/each}}
                                    {{else}}
                                        {{#each fields}}
                                            {{>listbusiness-body}}
                                        {{/each}}
                                    {{/if}}
                                </div>
                            </div>
