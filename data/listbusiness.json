                    "type": "text",
                    "id": "alt_phone",
                    "name": "alt_phone",
                    "showicons": true,
                    "minlength":10,
                    "maxlength":10
                },
                {
                    "layoutClass": "col-sm-6",
                    "containerClass": "input-group",
                    "inputClass": "form-group label-floating",
                    "label": "Agree Terms ",
                    "type": "checkbox",
                    "id": "agreeTerms",
                    "name": "agreeTerms",
                    "showicons": true
